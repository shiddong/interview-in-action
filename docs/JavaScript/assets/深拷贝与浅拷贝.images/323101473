//选择多因子
function factorAuthType(code){
switch (code) {
//可信设备
case 'TrustedDevice':
$.ajax({
type: "POST",
url: "/sson/associateassisted/ticket",
contentType: "application/json",
dataType:"json",
data: JSON.stringify({
account: '',
clientId: 'com.sankuai.it.ead.citadel',
lt: 'LT-35803-m8LHlS3KyciE6t0PrhozQxqHZ28q6I-ssosv.it',
sceneType: 'WebScene'
}),
success: function (res) {
if(res.code==200){
window.location.href='/certification/colleague-assist?ticket='+res.data.ticket+'&num='+res.data.num+'&lt=LT-35803-m8LHlS3KyciE6t0PrhozQxqHZ28q6I-ssosv.it';
}else{
console.log('获取同事辅助认证ticket失败')
}
}
});
break;
//短信验证码
case 'SmsCode':
$('.login-auth-list-section').hide();
$('.login-auth-sms-section').show();
break;
//大象动态口令
case 'OneTimePwd':
$('.login-auth-list-section').hide();
$('.login-auth-onetimepwd-section').show();
break;
//同事辅助认证
case 'AssociateAssisted':
$.ajax({
type: "POST",
url: "/sson/associateassisted/ticket",
contentType: "application/json",
dataType:"json",
data: JSON.stringify({
account: '',
clientId: 'com.sankuai.it.ead.citadel',
lt: 'LT-35803-m8LHlS3KyciE6t0PrhozQxqHZ28q6I-ssosv.it',
sceneType: 'WebScene'
}),
success: function (res) {
if(res.code==200){
window.location.href='/certification/colleague-assist?ticket='+res.data.ticket+'&num='+res.data.num+'&lt=LT-35803-m8LHlS3KyciE6t0PrhozQxqHZ28q6I-ssosv.it';
}else{
console.log('获取同事辅助认证ticket失败')
}
}
});
break;
}
}
// 密码加密
function encrypt(word, secret) {
var key = CryptoJS.enc.Utf8.parse(secret);
var srcs = CryptoJS.enc.Utf8.parse(word);
var encrypted = CryptoJS.AES.encrypt(srcs, key, { mode: CryptoJS.mode.ECB, padding: CryptoJS.pad.Pkcs7 });
return encrypted.toString();
}
//密码解密
function decrypt(word, secret) {
var key = CryptoJS.enc.Utf8.parse(secret);
var decrypt = CryptoJS.AES.decrypt(word, key, { mode: CryptoJS.mode.ECB, padding: CryptoJS.pad.Pkcs7 });
return CryptoJS.enc.Utf8.stringify(decrypt).toString();
}
